
{
  "rewrites": [
    {
      "source": "/.well-known/farcaster/app.json",
      "destination": "/farcaster.json"
    }
  ],
  "headers": [
    {
      "source": "/(?:farcaster\\.json|\\.well-known/farcaster/app\\.json)",
      "headers": [
        {
          "key": "Content-Type",
          "value": "application/json"
        }
      ]
    }
  ]
}
